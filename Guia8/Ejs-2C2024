#guia8 ej 8 4 #generar nros_al _azar (in cantidad:int, in desde:int, in hasta:int)->pila[int], se puede usar random. randint[desde, hasta] y la clase LifoQueue()
from queue import LifoQueue as Pila 7 
import random 
=p Pila() 
#p. put(1) #apilar 
#lemento = p.get() #desapilar 
#p.empty #bool, pregunta si es vacia 
def generar nros_al_azar(cantidad:int,desde:int,hasta:int)-> Pila[int]: 
    pila:Pila[int]= Pila() 
    for i in range(0,cantidad,1): #si lo empezaramos desde el 1 es cantidad +1 
       elem:int = random.randint(desde,hasta) 
       pila.put(elem) 
    return pila

#FUNCION PARA IMPRIMIR PILAS Y COLAS 
def impresoralp:Pila[int])-> None: #tbien podria devolver una lista 
    paux:Pila[int]= Pila()
    while not p.empty(): 
       elem:int = p.get() 
       print(elem) 
       paux.put(elem)
    while not paux.empty():
       i = paux.get()
       p.put(i)




cantidad = 5 
